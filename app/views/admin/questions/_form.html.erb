<h1> <%=question_header(@question) %></h1>
<%= form_with model: [:admin,@test, @question], local: true do |form| %>

  <p>
    <%= form.text_field :title, id: :question_title %>
  </p>

  <p><%= form.submit value="Сохранить"%></p>
<% end %>
<%= link_to 'Вернуться', admin_tests_path(@question.test_id) %>